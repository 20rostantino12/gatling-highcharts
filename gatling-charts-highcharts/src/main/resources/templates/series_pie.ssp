<%@ val serie: com.excilys.ebi.gatling.highcharts.series.PieSeries %>
<%@ val centerX: Int %>
<%@ val centerY: Int %>
<%@ val size: Int %>
type: 'pie'
, name: '<%=serie.name%>'
, data: [
<% serie.elements.foreach{ s => %><%=s%>,<% } %>
]
, center: [<%=centerX%>, <%=centerY%>]
, size: <%=size%>
, showInLegend: false
, dataLabels: { enabled: false }